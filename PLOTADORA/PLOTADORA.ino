int getimageindexfromhttpheader(char* httpheader) {
  bool find = false;
  int index = 0;
  while (!find && httpheader[index + 3] != '\0') {
    if (httpheader[index] == 'b' && httpheader[index + 1] == 'i' && httpheader[index + 2] == 'n' && httpheader[index + 3] == ':')
      find = true;
    index++;
  }
  if (find)
    return index + 4;
  else
    return -1;
}

String getbinaryimageline(char* httpheader, int* index) {
  bool find = false;
  String binaryimage = "";
  int i;
  for (i = 0; i < H_RES; i++) {
    binaryimage += httpheader[*index + i];
  }
  binaryimage[H_RES] = '\0';
  *index += H_RES;
  return binaryimage;
}

